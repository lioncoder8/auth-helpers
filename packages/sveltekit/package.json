    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean:all": "rimraf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supabase/auth-helpers.git"
  },
  "keywords": [
    "Supabase",
    "Auth",
    "Svelte Kit",
    "Svelte"
  ],
  "author": "Supabase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supabase/auth-helpers/issues"
  },
  "homepage": "https://github.com/supabase/auth-helpers/tree/main/packages/sveltekit#readme",
  "devDependencies": {
    "@supabase/supabase-js": "2.15.0",
    "@sveltejs/kit": "1.15.2",
    "svelte": "^3.54.0",
    "vite": "^4.0.0",
    "rimraf": "^4.1.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "tsup": "^6.5.0",
    "tsconfig": "workspace:*"
  },
  "dependencies": {
    "@supabase/auth-helpers-shared": "workspace:*"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.15.2",
    "@supabase/supabase-js": "^2.15.0"
  }
}
