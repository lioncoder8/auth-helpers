{
  "name": "@supabase/auth-helpers-sveltekit",
  "version": "0.9.4",
  "description": "A collection of framework specific Auth utilities for working with Supabase.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js"
  },
